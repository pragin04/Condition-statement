import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        int c=sc.nextInt();
        int greater;
        if(a>=b&&a>=c)
        {
            greater=a;
        }else if(b>=a&&b>=c){
            greater=b;
        }else{
            greater=c;
        }
        
        System.out.println("The greatest number is: " + greater);

}
}
